{
    "name": "@brainwav/cortexdx-core",
    "version": "0.1.0",
    "description": "Core types and utilities for CortexDx",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./utils/*": "./dist/utils/*",
        "./logging/*": "./dist/logging/*",
        "./config/*": "./dist/config/*",
        "./di/*": "./dist/di/*"
    },
    "scripts": {
        "build": "tsup",
        "clean": "rimraf dist",
        "lint": "biome lint src",
        "format": "biome format --write .",
        "test": "vitest run"
    },
    "dependencies": {
        "zod": "^3.22.4",
        "pino": "^10.1.0",
        "ajv": "^8.12.0",
        "ajv-formats": "^3.0.1"
    },
    "devDependencies": {
        "@types/node": "^22.19.1",
        "tsup": "^8.1.0",
        "typescript": "^5.6.3",
        "vitest": "^2.1.3",
        "rimraf": "^6.0.1"
    }
}