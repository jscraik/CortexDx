{
  "default": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": {
    "line_length": 120,
    "heading_line_length": 120,
    "code_block_line_length": 120,
    "code_blocks": false,
    "tables": false,
    "headings": false
  },
  "MD024": {
    "siblings_only": true
  },
  "MD025": true,
  "MD026": {
    "punctuation": ".,;:!"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD033": {
    "allowed_elements": ["details", "summary", "kbd", "sub", "sup"]
  },
  "MD041": true,
  "no-hard-tabs": true,
  "whitespace": true
}
